{
    "name": "Simple Manga Downloader.",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "content_scripts": [{
        "js": [
               "jquery.min.js",
               "jquery-ui.min.js",
               "popup.js"
              ],
        "css": [ "jquery-ui.css" ],
        "matches": [ "http://similarto-at-include-in-greasemonkey.com/*" ],
        "run_at": "document_end"
    }]
}